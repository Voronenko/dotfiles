// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "cmd+g",
    "command":"workbench.action.gotoLine"
  },
  {
    "key": "ctrl+w",
    "mac": "alt+up",
    "command": "editor.action.smartSelect.grow",
    "when": "editorTextFocus",
    "intellij": "Select successively increasing code blocks"
  },
  {
    "key": "ctrl+shift+w",
    "mac": "alt+down",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus",
    "intellij": "Decrease current selection to previous state"
  },
  {
    "key": "alt+shift+1",
    "command":"workbench.files.action.showActiveFileInExplorer"
  },
  {
    "key": "ctrl+y",
    "mac": "cmd+backspace",
    "command": "editor.action.deleteLines",
    "when": "editorTextFocus && !editorReadonly",
    "intellij": "Delete line at caret"
  },
  {
    "key": "f8",
    "mac": "f8",
    "command": "workbench.action.debug.stepOver",
    "when": "inDebugMode",
    "intellij": "Step over"
  },
  {
    "key": "f7",
    "mac": "f7",
    "command": "workbench.action.debug.stepInto",
    "when": "inDebugMode",
    "intellij": "Step into"
  },
  {
    "key": "shift+f8",
    "mac": "shift+f8",
    "command": "workbench.action.debug.stepOut",
    "when": "inDebugMode",
    "intellij": "Step out"
  },
  {
    "key": "shift+F10",
    "command":"workbench.action.debug.run",
    "when": "inDebugMode"
  },
  {
    "key": "alt+f9",
    "mac": "alt+f9",
    "command": "editor.debug.action.runToCursor",
    "when": "inDebugMode",
    "intellij": "Run to cursor"
  },
  {
    "key": "ctrl+f8",
    "mac": "cmd+f8",
    "command": "editor.debug.action.toggleBreakpoint",
    "when": "editorTextFocus",
    "intellij": "Toggle breakpoint"
  },
  {
    "key": "shift+F2",
    "command":"workbench.action.debug.stop",
    "when": "inDebugMode"
  },
  {
    "key": "f9",
    "mac": "cmd+alt+r",
    "command": "workbench.action.debug.continue",
    "when": "inDebugMode",
    "intellij": "Resume program"
  },

